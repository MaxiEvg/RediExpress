<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home_s3">


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/searchbar"
            android:fontFamily="@font/roboto_regular"
            android:hint="@string/search_services"
            android:paddingStart="12dp"
            android:textColorHint="@color/gray_soft"
            android:textSize="12sp"
            tools:ignore="RtlSymmetry,TouchTargetSizeCheck" />
    </com.google.android.material.textfield.TextInputLayout>

    <FrameLayout
        android:id="@+id/frameLayout5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="@drawable/profileinfo"
        app:layout_constraintEnd_toEndOf="@+id/textInputLayout"
        app:layout_constraintStart_toStartOf="@+id/textInputLayout"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingTop="15dp"
            android:paddingEnd="10dp"
            android:paddingBottom="15dp">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                app:srcCompat="@drawable/pfp"
                android:importantForAccessibility="no" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingStart="8dp"
                tools:ignore="RtlSymmetry">

                <TextView
                    android:id="@+id/helloname1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:text="@string/hello_name"
                    android:textColor="@color/white"
                    android:textSize="16sp" />

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        tools:ignore="UselessParent">

                        <TextView
                            android:id="@+id/wetrustyou"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_regular"
                            android:paddingTop="4dp"
                            android:text="@string/we_trust_you_are_having_a_great_time"
                            android:textColor="@color/gray_light"
                            android:textSize="12sp"
                            tools:ignore="TextContrastCheck" />

                    </TableRow>

                </TableLayout>

            </LinearLayout>

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0"
                app:srcCompat="@drawable/whitenotification"
                android:importantForAccessibility="no" />

        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout5"
        app:layout_constraintStart_toStartOf="@+id/frameLayout5"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout5">

        <TextView
            android:id="@+id/specialforyou"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/special_for_you"
            android:textColor="@color/secondary_color"
            android:textSize="14sp"
            tools:ignore="TextContrastCheck" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center|end"
            android:importantForAccessibility="no"
            app:srcCompat="@drawable/arrow_square_right"
            tools:ignore="ImageContrastCheck" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/ads"
        android:layout_width="0dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="@+id/frameLayout6"
        app:layout_constraintStart_toStartOf="@+id/frameLayout6"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout6">

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:ignore="UselessParent">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="64dp"
                android:orientation="horizontal"
                tools:ignore="DisableBaselineAlignment">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="13dp"
                    android:layout_weight="0">

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="79dp"
                        android:layout_height="37dp"
                        android:layout_gravity="center|start"
                        android:elevation="8dp"
                        android:fontFamily="@font/roboto_regular"
                        android:gravity="center"
                        android:text="@string/tech_meetup_coming_soon"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="12sp"
                        tools:ignore="TextSizeCheck" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|end"
                        android:elevation="8dp"
                        android:fontFamily="@font/roboto_regular"
                        android:paddingEnd="4dp"
                        android:paddingBottom="4dp"
                        android:text="@string/www_techmeetup"
                        android:textColor="@color/white"
                        android:textSize="5.7sp"
                        tools:ignore="RtlSymmetry,SmallSp" />

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/ad1"
                        android:importantForAccessibility="no" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="12dp"
                    android:layout_weight="0">

                    <TextView
                        android:id="@+id/loverevolution"
                        android:layout_width="79dp"
                        android:layout_height="37dp"
                        android:layout_gravity="center"
                        android:elevation="8dp"
                        android:fontFamily="@font/roboto_regular"
                        android:gravity="center"
                        android:text="@string/love_revolution"
                        android:textAlignment="center"
                        android:textColor="@color/secondary_color"
                        android:textSize="12sp"
                        tools:ignore="TextSizeCheck,TextContrastCheck" />

                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:elevation="4dp"
                        android:importantForAccessibility="no"
                        app:srcCompat="@drawable/orangebox"
                        tools:ignore="ImageContrastCheck" />

                    <ImageView
                        android:id="@+id/ad2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:importantForAccessibility="no"
                        app:srcCompat="@drawable/ad2"
                        tools:ignore="ImageContrastCheck" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="12dp"
                    android:layout_weight="0">

                    <TextView
                        android:id="@+id/thetext"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|start"
                        android:elevation="8dp"
                        android:fontFamily="@font/roboto_regular"
                        android:gravity="center"
                        android:paddingStart="15dp"
                        android:text="@string/the"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="12sp"
                        tools:ignore="RtlSymmetry" />

                    <TextView
                        android:id="@+id/genius"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|end"
                        android:elevation="8dp"
                        android:fontFamily="@font/roboto_regular"
                        android:paddingEnd="8dp"
                        android:text="@string/genius"
                        android:textColor="@color/white"
                        android:textSize="12sp"
                        tools:ignore="RtlSymmetry" />

                    <ImageView
                        android:id="@+id/ad3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/ad3"
                        android:importantForAccessibility="no" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="12dp"
                    android:layout_weight="0">

                    <TextView
                        android:id="@+id/cookedup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:elevation="8dp"
                        android:fontFamily="@font/roboto_regular"
                        android:gravity="center"
                        android:text="@string/cooked_up"
                        android:textAlignment="center"
                        android:textColor="#630202"
                        android:textSize="12sp" />

                    <ImageView
                        android:id="@+id/ad4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/ad4"
                        android:importantForAccessibility="no" />
                </FrameLayout>

            </LinearLayout>
        </HorizontalScrollView>
    </FrameLayout>

    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/roboto_medium"
        android:padding="10dp"
        android:text="@string/what_would_you_like_to_do"
        android:textColor="@color/primary_color"
        app:layout_constraintEnd_toEndOf="@+id/ads"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ads"
        app:layout_constraintTop_toBottomOf="@+id/ads" />

    <TableLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/customerscarebutton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/wholecustomercare"
                android:importantForAccessibility="no"
                android:longClickable="true"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/sendpackage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/wholesend"
                android:importantForAccessibility="no"
                android:longClickable="true"
                tools:ignore="SpeakableTextPresentCheck" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/fundwallet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/wholefundwallet"
                android:importantForAccessibility="no"
                android:longClickable="true"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/chatsbutton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/wholechat"
                android:importantForAccessibility="no"
                android:longClickable="true"
                tools:ignore="SpeakableTextPresentCheck" />

        </TableRow>

    </TableLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        app:elevation="2dp"
        app:itemIconTint="@color/gray_light"
        app:itemTextColor="@color/gray_light"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_navigation_menu" />

</androidx.constraintlayout.widget.ConstraintLayout>